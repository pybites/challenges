<script>
    $('.calculateFTC').on('click', function(){
        var sc = parseFloat($('.systemCost').val().replace(/(,|\$)/g,''));
        var ic = parseFloat($('.installCost').val().replace(/(,|\$)/g,''));
        var tc = parseFloat((sc+ic) * 0.3).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        $('.totalCredit').val(tc);
    })
