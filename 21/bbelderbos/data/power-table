<script>

    var GoogleReviews = (function () {
        "use strict";
        // instance stores a reference to this singleton (GoogleReviews)
        var instance;
        function init() {
            // this is a private function that only this instance can call
            //immediately invokes initReviews to get some data
            (function initReviews() {
                var service = new google.maps.places.PlacesService(reviewData);
                service.getDetails({
                    placeId: 'ChIJu7oUjljmzVQRztaAkJUioRo'
                }, function (place, status) {
                    if (status === google.maps.places.PlacesServiceStatus.OK) {
                        // if we receive data back call showReviews
                        showReviews(place);
                    }
                });
            })();
            function showReviews(data) {
                var templates = {};
                templates.googleReviews = null;
                templates.googleReviews = Handlebars.compile($('#google_reviews_template').html());
                // clear the reviews container
                var reviewContainer = jQuery('#review-container').html('');
                // if reviews are returned
                if (data != null) {
                    $.support.cors = true;
                    jQuery.ajax({
                        url:'https://www.googleapis.com/plus/v1/people/117801857904210967967?key=AIzaSyAO8Cv1zn4tLxPJMGqxqhN-P_FcAbLclxw',
                        type: 'GET',
                        contentType: "application/json; charset=utf-8",
                        crossDomain: true,
                        error: function (request, status, error) {
                            console.log(error);
                        },
                        success: function (placeData) {
                            templates.googleBusiness = null;
                            templates.googleBusiness = Handlebars.compile($('#google_business_template').html());
                            var businessContainer = jQuery('#business-container').html('');
                            var googleBusiness = placeData;
                            var place = {};
                            place.link = placeData.url;
                            place.name = placeData.displayName;
                            place.rating = data.rating + " out of 5 stars";
                            place.image = placeData.image.url.replace(/sz=50/, "sz=60");
                            var placeHtml = templates.googleBusiness(place);
                            businessContainer.html(placeHtml);
                        }
                    });
                    // create the objects for the template
                    var googleReviews = data.reviews;
                    var gReviews = [];
                    var r = 0;
                    //loop through all reviews
                    for (var i = 0; i < googleReviews.length; i++) {
                        // if good review counter = 3 break
                        // if rating is <= 3 break (don't show)
                        if (r === 3 || googleReviews[i].rating <= 3) {
                            continue;
                        }
                        var d = new Date(0);
                        var review = {};
                        if (googleReviews[i].profile_photo_url) {
                            review.image = googleReviews[i].profile_photo_url + "?sz=60";
                        } else {
                            review.image = "//ww3.wholesalesolar.com/review-photo.png";
                        }
                        review.author = googleReviews[i].author_name;
                        review.text = googleReviews[i].text;
                        review.date = jQuery.timeago(d.setUTCSeconds(googleReviews[i].time));
                        review.rating = googleReviews[i].rating + " out of 5 stars";
                        review.stars = {};
                        for (var j = 0; j < googleReviews[i].rating; j++) {
                            review.stars[j] = j
                        }
                        gReviews.push(review);
                        //increment the good review counter
                        r++
                    }
                    var reviewsContext = {reviews: gReviews};
                    var reviewsHtml = templates.googleReviews(reviewsContext);
                    reviewContainer.html(reviewsHtml);
                }
            } // end of private function showReviews();
            return {
                getReviews: function() {
                    return initReviews();
                }
            }
        } // end of GoogleReviews.init()
        return {
            // Get the Singleton instance if one exists
            // or create one if it doesn't
            // These are public functions accessible via GoogleReviews.nameOfFunction
            getInstance: function () {
                if ( !instance ) {
                    instance = init();
                }
                return instance;
            },
            reviewShowMore: function(thisReview) {
                var showmoreofthis = thisReview.parentNode.previousElementSibling,
                    showmoreofthisfade = showmoreofthis.childNodes[1];
                showmoreofthis.removeChild(showmoreofthisfade);
                $(showmoreofthis).animate({height: $(showmoreofthis).get(0).scrollHeight}, 400);
                $(thisReview).hide(400);
                dataLayer.push({
                    'event': 'GAEvent',
                    'eventCategory': "Google Reviews",
                    'eventAction': 'Clicked read more',
                    'eventLabel' : undefined,
                    'eventValue' : undefined
                });
            }
        };
    })();
</script>
