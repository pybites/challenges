	var cartLinkText = "View Cart / Checkout",
		cartLink = 'https://' + window.location.hostname + '/cart-checkout',
		cartLinkButton = $('#modalButtonViewCart');

	// for dealers
	if (wholesalesolar.dealer && wholesalesolar.dealer.loggedIn === true && $.cookie('dealerPortal.currentProjectId') != 'undefined') {
		var currentProjectId = $.cookie('dealerPortal.currentDealerProject'),

		cartLinkText = '<b class="fa fa-list"></b> View Project';
		cartLink =  'https://' + window.location.hostname + '/dealers/#/projects/' + currentProjectId;
		cartLinkButton.on('click', function (event) {
			event.preventDefault();
			wholesalesolar.dealer.goToProjectDetails()
		});
		cartLinkButton.addClass('dealer-purple');
	}

	$('#view-cart-modal').attr("href", cartLink);
	cartLinkButton.html(cartLinkText);
